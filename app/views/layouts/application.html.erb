<!DOCTYPE html>
<html>
  <head>
    <title>GearUpFe</title>

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "layouts/navbar" %>
    <% flash.each do |type, message| %>
    <div> <p><%= message %></p> </div>
    <% end %>
    <%= yield %>
  </body>
</html>
